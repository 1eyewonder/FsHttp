<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <GeneratePackageOnBuild>false</GeneratePackageOnBuild>
    <PackageId>SchlenkR.FsHttp</PackageId>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <Version>0.2.0</Version>
    <Authors>Ronald Schlenker</Authors>
    <Company />
    <Description>A lightweight HTTP/REST library for F#.</Description>
    <Copyright>Copyright 2018 Ronald Schlenker</Copyright>
    <PackageLicenseUrl>https://raw.githubusercontent.com/ronaldschlenker/FsHttp/master/LICENSE</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/ronaldschlenker/FsHttp</PackageProjectUrl>
    <PackageIconUrl>
    </PackageIconUrl>
    <PackageReleaseNotes>
      * Updated FSharp.Data to 3.0.0
      * More integration tests
      * Several breaking API changes (see Readme.md for current API)
      * Fix: ContentType is ignored when set explicitly
      * Feature: HTTP status code expectation
    </PackageReleaseNotes>
    <RepositoryUrl>https://github.com/ronaldschlenker/FsHttp.git</RepositoryUrl>
    <PackageTags>F# FSharp Http Rest</PackageTags>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <DefineConstants>DEBUG;TRACE</DefineConstants>
  </PropertyGroup>
  <ItemGroup>
    <Compile Include="FsHttp.Domain.fs">
      <PackagePath>.</PackagePath>
      <CopyToOutputDirectory>Never</CopyToOutputDirectory>
    </Compile>
    <Compile Include="FsHttp.Dsl.fs">
      <PackagePath>.</PackagePath>
      <CopyToOutputDirectory>Never</CopyToOutputDirectory>
    </Compile>
    <Compile Include="FsHttp.Runtime.fs">
      <PackagePath>.</PackagePath>
      <CopyToOutputDirectory>Never</CopyToOutputDirectory>
    </Compile>
    <Compile Include="FsHttp.Testing.fs" />
    <None Include="FsHttp.fsx">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
      <Pack>true</Pack>
      <PackagePath>lib/$(TargetFramework)</PackagePath>
    </None>
  </ItemGroup>
  <Import Project="..\..\.paket\Paket.Restore.targets" />
</Project>